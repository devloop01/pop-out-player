{
	"manifest_version": 2,
	"name": "Pop out player",
	"version": "1.0.0",
	"description": "Launch a pop out player in chrome for any youtube video",
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/watch?*"],
			"js": ["js/script.js"],
			"css": ["css/style.css"]
		}
	],
	"browser_action": {
  			"default_icon": "icons/icon.png"
	}
}
